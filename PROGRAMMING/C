#include<stdio.h>
int main()
{

    int i,n,num[100];

    printf("enter the value of n :");
    scanf("%d",&n);

    printf("original array\n : ");

    for(i=0;i<n;i++)
    {

        scanf("%d",&num[i]);
    }

    printf("\n");

      printf("revers array \n: ");

    for(i= n-1;i>=0;i--)
    {

        printf (" %d",num[i]);

    }


}
